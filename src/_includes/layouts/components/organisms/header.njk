<nav class="mf-header">
  <ul class="mf-header_links">
    {% for webpage in collections.webpage %}  
    <li class="mf-navlink" {% if webpage.url == page.url %}aria-current="true"{% endif %}><a href="{{webpage.url}}" class="mf-navlink_text">{{webpage.data.title}}</a></li>
    {% endfor %}
    <li class="mf-navlink"><button class="mf-navlink_text" id="submenuButton" role="button" aria-haspopup="true" aria-expanded="false" aria-label="mobile menu"><span>Menu</span><svg width="7" height="4" viewBox="0 0 7 4" fill="none" xmlns="http://www.w3.org/2000/svg" class="mf-navlink_arrow"><use xlink:href="#menu-arrow"/></svg></button></li>
    <li class="mf-navlink mf-toggle">
      <button id="darkmodeButton" class="mf-navlink_text" role="button" aria-label="toggle dark mode"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path class="light" fill="var(--fung-white-default)" d="M0 12c0 6.627 5.373 12 12 12s12-5.373 12-12-5.373-12-12-12-12 5.373-12 12zm2 0c0-5.514 4.486-10 10-10v20c-5.514 0-10-4.486-10-10z"/>
   <path class="dark" fill="var(--fung-accent-one-default)" d="M0 12c0 6.627 5.373 12 12 12s12-5.373 12-12-5.373-12-12-12-12 5.373-12 12zm2 0c0-5.514 4.486-10 10-10v20c-5.514 0-10-4.486-10-10z"/></svg></button>
    </li>
  </ul>
</nav>
<div class="mf-mobile" id="mobile-menu" data-open="false">
  <ul class="mf-mobile-links">
    {% for webpage in collections.webpage %}
      <li class="mf-navlink"><a href="{{webpage.url}}" class="mf-navlink_text">{{webpage.data.title}}</a></li>
    {% endfor %}
  </ul>
</div>
<div class="mf-logo">
  <a href="/">
    <span class="visually-hidden">Homepage</span>
    <svg xmlns="http://www.w3.org/2000/svg" width="90" height="40" fill="none" viewBox="0 0 180 80" aria-hidden="true">
      <use xlink:href="#text" />
      <use xlink:href="#dot" />
    </svg>
  </a>
</div>
{# {% endif %} #}